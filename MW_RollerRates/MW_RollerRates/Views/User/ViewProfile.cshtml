@model MW_RollerRates.Models.UserViewModel

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["Title"] = "Profile page";
}

<div class="content bg_white_block">
    <div class="row">
        <div class="col-md-6">
            <h1>Username : @Model.DisplayName</h1>
            <h2>Email : @Model.Email</h2>
            <h3>Description : @Model.Description</h3>
        </div>
    </div>
</div>